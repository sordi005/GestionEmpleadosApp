          <string notr="true">QFrame {
    /* Color de fondo del frame */
    background: #121212;
    
    /* Borde: ancho, estilo y color */
    border: 1px solid #333333;
    
    /* Esquinas redondeadas (radio en píxeles) */
    border-radius: 1px;
    
    /* Espacio interno entre el borde y el contenido */
    padding: 1px;
}

QGroupBox {
    /* Estilo de borde */
    border: 1px solid #a0a0a0;
    
    /* Radio de esquinas */
    border-radius: 3px;
    
    /* Margen superior para separar del título */
    margin-top: 10px;
    
    /* Espacio interno arriba del contenido */
    padding-top: 15px;
    
    /* Fondo con gradiente lineal (de arriba a abajo) */
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #f9f9f9, stop:1 #e0e0e0);
}

/* Estilo específico para el TÍTULO del GroupBox */
QGroupBox::title {
    subcontrol-origin: margin;  /* Posición relativa al margen */
    left: 10px;                /* Desplazamiento horizontal */
    padding: 0 5px;            /* Espacio interno (arriba/abajo, izquierda/derecha) */
    color: #505050;            /* Color del texto */
}


QPushButton {
